ros2 run ros_gz_bridge parameter_bridge /model/my_roboboat/joint/right_engine_propeller_joint/cmd_thrust@std_msgs/msg/Float64]gz.msgs.Double

ros2 topic pub /model/my_roboboat/joint/left_engine_propeller_joint/cmd_thrust std_msgs/msg/Float64 "data: 150"
ros2 topic pub /model/my_roboboat/joint/right_engine_propeller_joint/cmd_thrust std_msgs/msg/Float64 "data: 150"

