<?xml version="1.0"?>
<robot name="usv" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="usv">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <geometry>
        <mesh filename="package://usv_description/meshes/usv.dae" scale="0.1 0.1 0.1"/>
      </geometry>

    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.2"/>
      <geometry>
        <mesh filename="package://usv_description/meshes/usv.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="30.0"/>
      <inertia ixx="2.1" ixy="0" ixz="0" iyy="2.1" iyz="0" izz="2.1"/>
    </inertial>
  </link>

  <gazebo reference="world">
    <material>Gazebo/White</material>
    <gravity>true</gravity>
  </gazebo>

</robot>